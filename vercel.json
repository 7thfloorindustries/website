{
  "crons": [
    {
      "path": "/api/cron/snapshot",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/scrape",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/scrape/process",
      "schedule": "* 6 * * *"
    },
    {
      "path": "/api/creatorcore/sync",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/creatorcore/pending-hydrate",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/creatorcore/genre",
      "schedule": "30 */6 * * *"
    },
    {
      "path": "/api/reports/weekly",
      "schedule": "0 9 * * 1"
    }
  ],
  "rewrites": [
    {
      "source": "/broke/:path*",
      "has": [
        {
          "type": "host",
          "value": "brokedown.app"
        }
      ],
      "destination": "/broke/:path*"
    },
    {
      "source": "/api/:path*",
      "has": [
        {
          "type": "host",
          "value": "brokedown.app"
        }
      ],
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "brokedown.app"
        }
      ],
      "destination": "/broke/:path*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "brokedown.app"
        }
      ],
      "destination": "/broke"
    }
  ]
}
